{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "services": {
      "type": "library",
      "root": "libs/services",
      "entryFile": "index",
      "sourceRoot": "libs/services/src",
      "compilerOptions": {
        "tsConfigPath": "libs/services/tsconfig.lib.json"
      }
    },
    "auth-repository": {
      "type": "library",
      "root": "libs/auth-repository",
      "entryFile": "index",
      "sourceRoot": "libs/auth-repository/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-repository/tsconfig.lib.json"
      }
    },
    "auth-resolver": {
      "type": "library",
      "root": "libs/auth-resolver",
      "entryFile": "index",
      "sourceRoot": "libs/auth-resolver/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-resolver/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}