{
  "sourceRoot": "libs/shared/services/prisma-services/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/shared/services/prisma-services/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/shared/services/prisma-services"
      ],
      "options": {
        "jestConfig": "libs/shared/services/prisma-services/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
